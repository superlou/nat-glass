<svg version="1.1" width="640" height="400"
     viewBox={{this.view_box}}>
    <circle cx="0" cy="0" r={{this.plate_radius}} fill="#c0e0f0"/>
    <RadialPattern @rect_rotations={{this.rect_rotations}}>
        <rect x={{this.rect_x}} y={{this.rect_y}} width={{@rect_w}} height={{@rect_h}}
              fill="#f0a04060"/>
    </RadialPattern>
</svg>